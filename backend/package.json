{
    "name": "backend",
    "private": true,
    "version": "0.0.0",
    "scripts": {
        "start": "node dist/server.js",
        "tsc-build": "tsc",
        "clean": "rimraf ../dist",
        "build:server": "cross-env NODE_ENV=production npm run clean && tsc -p tsconfig.server.json",
        "dev:server": "ts-node-dev --respawn --transpile-only --project ./tsconfig.server.json src/server.ts"
    },
    "dependencies": {
        "bcrypt": "^5.1.1",
        "connect-mongo": "^5.1.0",
        "cors": "^2.8.5",
        "dotenv": "^16.4.5",
        "express": "^4.19.2",
        "express-session": "^1.18.0",
        "mongodb": "^6.6.1",
        "mongoose": "^8.4.1",
        "mysql": "^2.18.1",
        "node-geocoder": "^4.4.0",
        "nodemailer": "^6.9.14",
        "supertest": "^7.0.0",
        "@types/bcrypt": "^5.0.2",
        "@types/cors": "^2.8.17",
        "@types/dotenv": "^8.2.0",
        "@types/express": "^4.17.21",
        "@types/express-session": "^1.18.0",
        "@types/mongoose": "^5.11.97",
        "@types/node": "^20.14.10",
        "@types/node-geocoder": "^4.2.6",
        "@types/nodemailer": "^6.4.15",
        "@types/supertest": "^6.0.2"
    },
    "devDependencies": {
        "@types/jest": "^29.5.12",
        "cross-env": "^7.0.3",
        "eslint": "^8.57.0",
        "eslint-plugin-react": "^7.34.1",
        "nodemon": "^3.1.0",
        "ts-node": "^10.9.2",
        "ts-node-dev": "^2.0.0",
        "typescript": "^5.5.3"
    }
}
